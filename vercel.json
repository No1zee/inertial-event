{
  "version": 2,
  "rewrites": [
    {
      "source": "/api/keygen/:path*",
      "destination": "/keygen-server/server.js"
    },
    {
      "source": "/api/:path*",
      "destination": "/backend/src/index.ts"
    },
    {
       "source": "/keygen-admin/:path*",
       "destination": "/keygen-admin/:path*"
    }
  ]
}
