{
  "version": 2,
  "functions": {
    "keygen-server/server.js": {
      "runtime": "@vercel/node"
    },
    "backend/src/index.ts": {
      "runtime": "@vercel/node"
    }
  },
  "rewrites": [
    {
      "source": "/api/keygen/:path*",
      "destination": "/keygen-server/server.js"
    },
    {
      "source": "/api/:path*",
      "destination": "/backend/src/index.ts"
    }
  ]
}
