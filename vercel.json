{
    "version": 2,
    "functions": {
        "api/keygen/index.mjs": {
            "memory": 256,
            "maxDuration": 10
        }
    },
    "rewrites": [
        {
            "source": "/api/keygen/ping",
            "destination": "/api/keygen/index.mjs"
        },
        {
            "source": "/api/keygen/api/:path*",
            "destination": "/api/keygen/index.mjs"
        },
        {
            "source": "/api/keygen/:path*",
            "destination": "/api/keygen/index.mjs"
        },
        {
            "source": "/api/keygen",
            "destination": "/api/keygen/index.mjs"
        }
    ]
}
