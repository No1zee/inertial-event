{
  "version": 2,
  "rewrites": [
    {
      "source": "/api/keygen/:path*",
      "destination": "/api/keygen"
    },
    {
      "source": "/api/:path*",
      "destination": "/api/main"
    }
  ],
  "redirects": [
    {
      "source": "/admin-panel",
      "destination": "/keygen-admin/index.html",
      "permanent": false
    },
    {
      "source": "/keygen-dashboard",
      "destination": "/keygen-admin/index.html",
      "permanent": false
    }
  ],
  "cleanUrls": true,
  "trailingSlash": false
}
