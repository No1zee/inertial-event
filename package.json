{
    "name": "inertial-event",
    "version": "0.1.0",
    "private": true,
    "main": "electron/main.js",
    "scripts": {
        "dev": "cross-env PORT=3000 next dev -H 0.0.0.0",
        "build": "next build",
        "start": "next start",
        "lint": "next lint",
        "format": "prettier --write \"src/**/*.{ts,tsx}\"",
        "type-check": "tsc --noEmit",
        "test": "jest",
        "test:watch": "jest --watch",
        "analyze": "ANALYZE=true next build",
        "electron-dev": "concurrently -k \"npm run dev\" \"npm run server\" \"wait-on http://localhost:3000 http://localhost:5000 && electron .\"",
        "server": "cross-env BACKEND_PORT=5000 npx tsx backend/src/index.ts",
        "electron-build": "next build && electron-builder",
        "electron": "electron ."
    },
    "dependencies": {
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "next": "^14.1.0",
        "@tanstack/react-query": "^5.0.0",
        "@tanstack/react-query-devtools": "^5.0.0",
        "zustand": "^4.4.0",
        "framer-motion": "^10.16.0",
        "axios": "^1.6.0",
        "react-hook-form": "^7.48.0",
        "hls.js": "^1.4.0",
        "dash.js": "^4.5.0",
        "tailwindcss": "^3.3.0",
        "clsx": "^2.0.0",
        "tailwind-merge": "^2.2.0",
        "next-themes": "^0.2.1",
        "zustand-persist": "^1.1.0",
        "@headlessui/react": "^2.2.9",
        "@heroicons/react": "^2.2.0",
        "@radix-ui/react-dialog": "^1.1.15",
        "@vidstack/react": "^1.0.0",
        "bcrypt": "^6.0.0",
        "cors": "^2.8.5",
        "dotenv": "^17.2.3",
        "electron-serve": "^1.3.0",
        "express": "^5.2.1",
        "googleapis": "^129.0.0",
        "jsonwebtoken": "^9.0.3",
        "lucide-react": "^0.300.0",
        "mongoose": "^9.1.3",
        "tailwindcss-animate": "^1.0.7",
        "ts-node": "^10.9.2",
        "webtorrent": "^2.0.0",
        "zod": "^3.22.0"
    },
    "devDependencies": {
        "typescript": "^5.2.0",
        "@types/node": "^20.0.0",
        "@types/react": "^18.2.0",
        "@types/react-dom": "^18.2.0",
        "autoprefixer": "^10.4.0",
        "postcss": "^8.4.0",
        "prettier": "^3.0.0",
        "eslint": "^8.50.0",
        "eslint-config-next": "^14.0.0",
        "@testing-library/react": "^14.0.0",
        "@testing-library/jest-dom": "^6.0.0",
        "jest": "^29.0.0",
        "jest-environment-jsdom": "^29.0.0",
        "concurrently": "^8.0.0",
        "cross-env": "^7.0.0",
        "electron": "^28.0.0",
        "electron-builder": "^24.9.0",
        "tsx": "^4.21.0",
        "wait-on": "^7.0.0"
    }
}